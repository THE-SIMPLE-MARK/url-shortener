{
	"name": "my-t3-boilerplate",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "NODE_OPTIONS=\"--no-deprecation\" next dev --turbopack --experimental-https --port 3000",
		"start": "next start",
		"start:dev": "NODE_OPTIONS=\"--no-deprecation\" SKIP_ENV_VALIDATION=true VERCEL_URL=localhost:3000 next start",
		"lint": "TIMING=1 eslint . --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write \"**/*.{js,ts,tsx,md,mdc,json}\"",
		"db:gen": "prisma generate --no-hints",
		"db:gen:watch": "prisma generate --no-hints --watch",
		"db:push": "prisma db push",
		"db:reset": "prisma db push --force-reset",
		"db:dev": "prisma studio --browser none",
		"db:dev:init": "cd scripts && docker compose up -d",
		"postinstall": "prisma generate --no-hints"
	},
	"dependencies": {
		"@prisma/client": "^6.18.0",
		"@t3-oss/env-nextjs": "^0.13.8",
		"@tanstack/react-query": "^5.90.6",
		"@trpc/client": "^11.7.1",
		"@trpc/react-query": "^11.7.1",
		"@trpc/server": "^11.7.1",
		"@trpc/tanstack-react-query": "^11.7.1",
		"dotenv": "^17.2.3",
		"next": "^16.0.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"server-only": "^0.0.1",
		"superjson": "^2.2.5",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.0",
		"@next/eslint-plugin-next": "^16.0.1",
		"@tailwindcss/postcss": "^4.1.16",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"eslint": "^9.39.0",
		"eslint-config-next": "^16.0.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"prisma": "^6.18.0",
		"tailwindcss": "^4.1.16",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"packageManager": "pnpm@10.15.1"
}
